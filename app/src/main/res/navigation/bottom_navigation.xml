        android:name="com.example.prm392.Login"
        android:label="activity_login"
        tools:layout="@layout/activity_login" />
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.prm392.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" />
    <fragment
        android:id="@+id/managerFragment"
        android:name="com.example.prm392.ManagerFragment"
        android:label="fragment_manager"
        tools:layout="@layout/fragment_manager" />
    <fragment
        android:id="@+id/othersFragment"
        android:name="com.example.prm392.UserManagerFragment"
        android:label="fragment_others"
        tools:layout="@layout/fragment_user_manager" />

    <fragment
        android:id="@+id/profileFragment"
        android:name="com.example.prm392.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" />
